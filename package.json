{
  "name": "shipment-trafas",
  "version": "0.1.0",
  "description": "first dev of shipment app.",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss build src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "react-scripts start",
    "prebuild": "npm run tailwind:build",
    "build": "react-scripts build",
    "cz": "git-cz",
    "release": "release-it",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "@supabase/supabase-js": "^1.13.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@tinymce/tinymce-react": "^3.12.6",
    "@windmill/react-ui": "0.3.1",
    "chart.js": "^2.9.4",
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "fuse.js": "^6.4.6",
    "html-to-text": "^7.1.1",
    "normalize-url": "^6.1.0",
    "react": "^16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-datepicker": "^4.1.1",
    "react-dates": "^21.8.0",
    "react-datetime": "^3.0.4",
    "react-day-picker": "^7.4.10",
    "react-dom": "^16.13.1",
    "react-epic-spinners": "^0.5.0",
    "react-focus-lock": "2.4.0",
    "react-hook-form": "^7.8.1",
    "react-hot-toast": "^2.0.0",
    "react-html-parser": "^2.0.2",
    "react-redux": "^7.2.4",
    "react-roman": "^2.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-transition-group": "4.4.1",
    "redux": "^4.1.0",
    "romanjs": "^2.0.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^3.0.0",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^10.2.6",
    "commitizen": "^4.2.4",
    "cross-env": "7.0.2",
    "cssnano": "^5.0.6",
    "cz-conventional-changelog": "3.2.0",
    "postcss-cli": "^8.3.1",
    "react-axe": "3.5.2",
    "release-it": "^14.8.0",
    "tailwindcss": "^2.2.4"
  },
  "keywords": [],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "react",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
